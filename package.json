{
    "name": "massive-infinity-assessment",
    "version": "1.0.0",
    "description": "A simple RBAC app",
    "main": "app.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "eslint . -ext .ts",
        "lint-and-fix": "eslint . -ext .ts -fix",
        "build": "rimraf ./build && npx tsc",
        "dev": "nodemon src/app.ts",
        "key-pair": "node build/generateKeyPair.js",
        "setup:migration": "node build/scripts/migration.js",
        "setup:seed": "npm run key-pair && node build/scripts/seeder.js",
        "setup": "npm run setup:db && npm run setup:tables",
        "start": "npm run key-pair && node build/app.js"
    },
    "author": "Krishant",
    "license": "ISC",
    "dependencies": {
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-fileupload": "^1.4.0",
        "joi": "^17.7.0",
        "jsonwebtoken": "^8.5.1",
        "mysql2": "^2.3.3",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.0",
        "sequelize": "^6.25.5"
    },
    "devDependencies": {
        "@types/express": "^4.17.14",
        "@types/express-fileupload": "^1.4.1",
        "@types/joi": "^17.2.3",
        "@types/jsonwebtoken": "^8.5.9",
        "@types/node": "^18.11.9",
        "@types/passport": "^1.0.11",
        "@types/passport-jwt": "^3.0.7",
        "@types/sequelize": "^4.28.14",
        "@typescript-eslint/eslint-plugin": "^5.42.1",
        "@typescript-eslint/parser": "^5.42.1",
        "eslint": "^8.27.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "nodemon": "^2.0.20",
        "prettier": "^2.7.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.4"
    }
}
